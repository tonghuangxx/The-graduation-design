<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="fee" extends="default" namespace="/fee">
		<action name="list" class="com.dlts.fee.action.FeeAction" method="list">
			<result name="success">/index.jsp</result>
		</action>
		<action name="listData" class="com.dlts.fee.action.FeeAction" method="listData">
			<result name="success">/fee/fee_list.jsp</result>
		</action>
		<action name="sort" class="com.dlts.fee.action.FeeAction" method="sort">
			<result name="success">/fee/list.jsp</result>
		</action>
	</package>
</struts>        