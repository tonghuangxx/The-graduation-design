<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="bill" extends="default" namespace="/bill">
		<action name="listData" class="com.dlts.bill.action.BillAction" method="listData">
			<result name="success">/bill/bill_list.jsp</result>
		</action>
		<action name="search" class="com.dlts.bill.action.BillAction" method="search">
			<result name="success">/bill/list.jsp</result>
		</action>
		<action name="getBillItem" class="com.dlts.bill.action.BillAction" method="getBillItem">
			<result name="success">/bill/bill_item.jsp</result>
		</action>
	</package>
</struts>        